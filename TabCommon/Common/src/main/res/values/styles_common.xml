<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="SystemUISpace">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="isConsumeInsets">false</item>
    </style>

    <style name="SystemUISpace.Top">
        <item name="android:layout_width">match_parent</item>
        <item name="systemUI">top</item>
    </style>

    <style name="SystemUISpace.Top.Consumed">
        <item name="isConsumeInsets">true</item>
    </style>

    <style name="SystemUISpace.Left">
        <item name="android:layout_height">match_parent</item>
        <item name="systemUI">left</item>
    </style>

    <style name="SystemUISpace.Left.Consumed">
        <item name="isConsumeInsets">true</item>
    </style>

    <style name="SystemUISpace.Right">
        <item name="android:layout_height">match_parent</item>
        <item name="systemUI">right</item>
    </style>

    <style name="SystemUISpace.Right.Consumed">
        <item name="isConsumeInsets">true</item>
    </style>

    <style name="SystemUISpace.Bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="systemUI">bottom</item>
    </style>

    <style name="SystemUISpace.Bottom.Consumed">
        <item name="isConsumeInsets">true</item>
    </style>


</resources>